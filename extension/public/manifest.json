{
  "manifest_version": 3,
  "name": "MeetCode",
  "version": "1.0",
  "description": "Transform LeetCode practice into a collaborative experience!",
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://leetcode.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}